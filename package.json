{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run tsc",
    "migration:revert": "\"./node_modules/.bin/typeorm\" migration:revert",
    "migration:run": "\"./node_modules/.bin/typeorm\" migration:run",
    "new-migration": "\"./node_modules/.bin/typeorm\" migration:generate --name",
    "start": "concurrently \"npm run tsc:w\" nodemon",
    "start:dev": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "node dist/index.js",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "watch:tsc": "tsc-watch --onSuccess \"node dist/index.js\""
  },
  "dependencies": {
    "@tsed/ajv": "^6.98.1",
    "@tsed/common": "^6.98.1",
    "@tsed/core": "^6.98.1",
    "@tsed/di": "^6.98.1",
    "@tsed/exceptions": "^6.98.1",
    "@tsed/json-mapper": "^6.98.1",
    "@tsed/passport": "^6.98.1",
    "@tsed/platform-express": "^6.98.1",
    "@tsed/schema": "^6.98.1",
    "@tsed/socketio": "^6.98.1",
    "@tsed/swagger": "^6.98.1",
    "@tsed/typeorm": "^6.98.1",
    "ajv": "^8.6.3",
    "bent": "^7.3.12",
    "body-parser": "1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "decimal.js": "^10.3.1",
    "express": "^4.17.1",
    "file-type": "^16.5.3",
    "form-data": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "socket.io": "^4.2.0",
    "typeorm": "^0.2.37",
    "typeorm-naming-strategies": "^2.0.0",
    "tsc-watch": "^4.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bent": "^7.3.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/form-data": "^2.5.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.9.6",
    "@types/passport": "^1.0.7",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/uuid": "^8.3.1",
    "@tsed/cli-plugin-passport": "3.11.5",
    "@tsed/cli-plugin-typeorm": "3.11.5",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "typescript": "^4.4.3",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0"
  },
  "tsed": {
    "packageManager": "npm",
    "convention": "default"
